{
  "name": "nlp2d",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run train; rm ./script/bundle.js; budo ./script/nlp.js:script/bundle.js --wg **/*.{html,css,js} --live",
    "build": "npm run train; browserify ./script/index.js | terser --compress --mangle > ./script/bundle.js",
    "train": "node ./script/nlp/train.js"
  },
  "author": "sglkc",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^17.0.0",
    "budo": "^11.8.4",
    "terser": "^5.16.0"
  },
  "dependencies": {
    "@nlpjs/core": "^4.23.4",
    "@nlpjs/lang-en": "^4.23.4",
    "@nlpjs/lang-id": "^4.23.4",
    "@nlpjs/lang-ja": "^4.24.0",
    "@nlpjs/nlp": "^4.23.5"
  }
}
